/* pages/register/register.wxss */
.middle-user-block{
  margin: 0 32rpx;
  background: white;
  border-radius: 25rpx;
  padding: 32rpx;
  box-shadow: 0 6rpx 2rpx #aaa;
}
.get-btn{
  border-radius: 25rpx;
  padding: 8rpx 24rpx;
  height: 40rpx;
  line-height: 40rpx;
  color: white;
  background: #ffa41b;
  font-size: 24rpx;
  box-shadow: 0px 6rpx 4rpx rgba(255,164,27,0.5);
}
.get-btn2{
  border-radius: 25rpx;
  padding: 8rpx 24rpx;
  height: 40rpx;
  line-height: 40rpx;
  color: white;
  background: #ccc;
  font-size: 24rpx;
  box-shadow: 0px 6rpx 4rpx #eee;
}
.get-btn-hover{
  transform: translateY(6rpx);
  box-shadow: 0px 0rpx 0rpx rgba(255,164,27,0.5);

}